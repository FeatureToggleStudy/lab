{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "prebuild": "rimraf dist",
    "build": "webpack --mode production --config webpack.config.production.js",
    "postbuild": "copyfiles index.html dist",
    "node-dev": "npx ts-node-dev --respawn --transpileOnly src/nodeStuff.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "hmr",
    "webpack",
    "typescript"
  ],
  "author": "Matt Erhart",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@types/pdfjs-dist": "^0.1.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/styled-components": "^4.1.4",
    "babel-runtime": "^6.26.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "pdfjs-dist": "^2.0.943",
    "react-hot-loader": "^4.6.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  }
}
